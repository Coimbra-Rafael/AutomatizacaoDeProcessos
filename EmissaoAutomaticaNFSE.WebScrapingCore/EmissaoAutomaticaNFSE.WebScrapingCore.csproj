      <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.4" />
      <PackageReference Include="Selenium.Support" Version="4.20.0" />
      <PackageReference Include="Selenium.WebDriver" Version="4.31.0" />
    </ItemGroup>

</Project>